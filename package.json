{
  "name": "hospital-management-system",
  "version": "1.0.0",
  "description": "A comprehensive hospital management system with appointment scheduling, patient management, and medical records",
  "main": "server/api/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start",
    "vercel-build": "cd client && npm run build"
  },
  "keywords": [
    "hospital",
    "management",
    "healthcare",
    "appointments",
    "patients",
    "doctors",
    "medical-records"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/hospital-management-system.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/hospital-management-system/issues"
  },
  "homepage": "https://github.com/yourusername/hospital-management-system#readme"
}
